<configuration>
  <system.web>

    <webParts>
      <personalization>
        <authorization>
          <!--Permiti que todos os usuários façam tudo-->
          <allow users="*" verbs="enterSharedScope"/>
        </authorization>
      </personalization>
    </webParts>

    <compilation debug="true" targetFramework="4.0"/>
  </system.web>
</configuration>